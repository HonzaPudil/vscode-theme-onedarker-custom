{
  "name": "Atom One Darker Contrast",
  "type": "dark",
  "colors": {
    "sideBar.background": "#21252b",
    "editorGroupHeader.tabsBackground": "#21252b",
    "tab.activeBackground": "#14161A",
    "tab.inactiveBackground": "#21252b",
    "activityBar.background": "#21252b",
    "editor.background": "#14161A",
    "editor.foreground": "#9DA5B4",
    "editorLineNumber.foreground": "#636d83",
    "editorCursor.foreground": "#528bff",
    "editor.selectionBackground": "#3e4451",
    "editor.lineHighlightBackground": "#99bbff0a"
  },
  "tokenColors": [
    {
      "name": "Other: Variables",
      "scope": ["variable.other.constant.js", "variable.other.readwrite.js"],
      "settings": { "fontStyle": "normal", "foreground": "#CACED6" }
    },
    {
      "name": "Other: Object Literal Quoted Key",
      "scope": ["string.quoted.double.js", "string.quoted.single.js"],
      "settings": { "fontStyle": "normal", "foreground": "#95D567" }
    },
    {
      "name": "Other: Object Literal Key",
      "scope": ["meta.object-literal.key.js"],
      "settings": { "fontStyle": "normal", "foreground": "#FD4F5C" }
    },
    {
      "name": "Other:  Function definition",
      "scope": [
        "meta.definition.function.js entity.name.function.js",
        "meta.function-call.js entity.name.function.js"
      ],
      "settings": { "fontStyle": "normal", "foreground": "#3DB2FF" }
    },
    {
      "name": "Other: Object literal method",
      "scope": ["entity.name.function.js"],
      "settings": { "fontStyle": "normal", "foreground": "#FD4F5C" }
    },
    {
      "name": "Other: Function attribute",
      "scope": ["variable.parameter.js"],
      "settings": { "fontStyle": "normal", "foreground": "#CACED6" }
    },
    {
      "name": "String: Punctuation",
      "scope": [
        "string.quoted.double punctuation",
        "string.quoted.single punctuation"
      ],
      "settings": { "fontStyle": "normal", "foreground": "#95D567" }
    },
    {
      "name": "Other: Punctuation separator, generator asterisk",
      "scope": [
        "punctuation.separator.key-value.js",
        "keyword.generator.asterisk.js"
      ],
      "settings": { "fontStyle": "normal", "foreground": "#3BCBDD" }
    },
    {
      "name": "Other: Punctuation separators",
      "scope": [
        "punctuation.separator.comma.js",
        "punctuation.accessor.js",
        "punctuation.terminator.statement.js"
      ],
      "settings": { "fontStyle": "normal", "foreground": "#CACED6" }
    },
    {
      "name": "Other: Support class",
      "scope": ["support.class.js"],
      "settings": { "fontStyle": "normal", "foreground": "#FFBC3D" }
    },
    {
      "name": "Other: Meta method declaratio",
      "scope": ["meta.method.declaration.js storage.type.js"],
      "settings": { "fontStyle": "normal", "foreground": "#3DB2FF" }
    },
    {
      "name": "Other: Support variables / console",
      "scope": ["support.variable.property.js", "support.class.console.js"],
      "settings": { "fontStyle": "normal", "foreground": "#FD4F5C" }
    },
    {
      "name": "Other: Support function / console",
      "scope": ["support.function.console.js"],
      "settings": { "fontStyle": "normal", "foreground": "#3DB2FF" }
    },
    {
      "name": "Other: Puncuation definition parameters/brackets/brace",
      "scope": [
        "punctuation.definition.parameters.begin.js",
        "punctuation.definition.parameters.end.js",
        "punctuation.definition.binding-pattern.object.js",
        "punctuation.definition.binding-pattern.array.js",
        "punctuation.definition.block.js",
        "meta.brace.round.js",
        "meta.brace.square.js"
      ],
      "settings": { "fontStyle": "normal", "foreground": "#CACED6" }
    },
    {
      "name": "Other: Booleans and numbers",
      "scope": [
        "constant.language.boolean",
        "constant.language.null.js",
        "constant.numeric.decimal.js"
      ],
      "settings": { "fontStyle": "normal", "foreground": "#EC994B" }
    },
    {
      "name": "Other: Support functions",
      "scope": ["support.function.js"],
      "settings": { "fontStyle": "normal", "foreground": "#3BCBDD" }
    },
    {
      "name": "Other: Regexp punctuation and bracket expression",
      "scope": [
        "punctuation",
        "constant.other.character-class.range.regexp",
        "constant.other.character-class.set.regexp"
      ],
      "settings": { "fontStyle": "normal", "foreground": "#3BCBDD" }
    },
    {
      "name": "Other: Regexp control anchors",
      "scope": ["keyword.control.anchor.regexp"],
      "settings": { "fontStyle": "normal", "foreground": "#EC994B" }
    },
    {
      "name": "Other: Regexp quantifiers",
      "scope": ["string.regexp.js"],
      "settings": { "fontStyle": "normal", "foreground": "#EC994B" }
    },
    {
      "name": "Other: Function storage",
      "scope": ["storage.type"],
      "settings": { "fontStyle": "normal", "foreground": "#D45FF6" }
    },
    {
      "name": "Other: Keyword controls",
      "scope": ["keyword.control"],
      "settings": { "fontStyle": "normal", "foreground": "#D45FF6" }
    },
    {
      "name": "Other: Promise",
      "scope": ["entity.name.type.js"],
      "settings": { "fontStyle": "normal", "foreground": "#FFBC3D" }
    },
    {
      "name": "Other: Keyword operators",
      "scope": ["keyword.operator"],
      "settings": { "fontStyle": "normal", "foreground": "#D45FF6" }
    },
    {
      "name": "Other: Keyword operator comparison / assignment",
      "scope": [
        "keyword.operator.comparison.js",
        "keyword.operator.assignment.js"
      ],
      "settings": { "fontStyle": "normal", "foreground": "#3BCBDD" }
    },
    {
      "name": "Comment: Line and Block comments",
      "scope": [
        "comment.line",
        "comment.block",
        "punctuation.definition.comment.js"
      ],
      "settings": { "fontStyle": "normal", "foreground": "#40454E" }
    },
    {
      "name": "Other: Support type module",
      "scope": ["support.type.object.module.js"],
      "settings": { "fontStyle": "normal", "foreground": "#FD4F5C" }
    },
    {
      "name": "Other: Class name",
      "scope": ["entity.name.type.class.js"],
      "settings": { "fontStyle": "normal", "foreground": "#FFBC3D" }
    },
    {
      "name": "Other: Template string",
      "scope": ["punctuation.definition.string.template", "string.template.js"],
      "settings": { "fontStyle": "normal", "foreground": "#95D567" }
    },
    {
      "name": "Other: Template expression",
      "scope": ["punctuation.definition.template-expression"],
      "settings": { "fontStyle": "normal", "foreground": "#BE5046" }
    },
    {
      "name": "Other: Template expression object",
      "scope": ["meta.embedded.line.js variable.other.object.js"],
      "settings": { "fontStyle": "normal", "foreground": "#CACED6" }
    },
    {
      "name": "Other: Other object property",
      "scope": ["variable.other.property.js"],
      "settings": { "fontStyle": "normal", "foreground": "#FD4F5C" }
    },
    {
      "name": "Other: Imports",
      "scope": [
        "variable.other.readwrite.alias.js",
        "variable.other.object.js"
      ],
      "settings": { "fontStyle": "normal", "foreground": "#FFBC3D" }
    },
    {
      "name": "Other: Function call object / meta block object / array litteral object",
      "scope": [
        "meta.block.js variable.other.object.js",
        "meta.function-call.js variable.other.object.js",
        "meta.array.literal.js variable.other.object.js"
      ],
      "settings": { "fontStyle": "normal", "foreground": "#CACED6" }
    },
    {
      "name": "Other: Array litteral readwrite",
      "scope": [
        "meta.object.member.js meta.array.literal.js variable.other.readwrite.js"
      ],
      "settings": { "fontStyle": "normal", "foreground": "#FFBC3D" }
    },
    {
      "name": "Other: Spread operators",
      "scope": ["keyword.operator.spread.js"],
      "settings": { "fontStyle": "normal", "foreground": "#3BCBDD" }
    },
    {
      "name": "Other: JSON punctuation",
      "scope": [
        "punctuation.definition.dictionary.begin.json",
        "punctuation.definition.dictionary.end.json",
        "punctuation.definition.array.begin.json",
        "punctuation.definition.array.end.json",
        "punctuation.separator.dictionary.key-value.json",
        "punctuation.separator.dictionary.pair.json"
      ],
      "settings": { "fontStyle": "normal", "foreground": "#CACED6" }
    },
    {
      "name": "String: JSON property name and punctuation",
      "scope": [
        "support.type.property-name.json punctuation",
        "support.type.property-name.json"
      ],
      "settings": { "fontStyle": "normal", "foreground": "#FD4F5C" }
    },
    {
      "name": "String: JSON string and punctuation",
      "scope": ["meta.structure.dictionary.value.json"],
      "settings": { "fontStyle": "normal", "foreground": "#95D567" }
    },
    {
      "name": "String: JSON string and punctuation",
      "scope": ["constant.numeric.json", "constant.language.json"],
      "settings": { "fontStyle": "normal", "foreground": "#EC994B" }
    }
  ]
}
